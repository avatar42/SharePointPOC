<?xml version='1.0' encoding='utf-8'?>
<widget id="com.dea42.sharepointpoc" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>SharePointPOC</name>
    <description>
        A sample Apache Cordova POC application for accessing the SharePoint API
    </description>
    <author email="app_sharepointPOC@m42.biz" href="http://dea42.com">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-x-toast" spec="https://github.com/EddyVerbruggen/Toast-PhoneGap-Plugin.git" />
    <plugin name="cordova-plugin-ms-adal" spec="^0.10.1" />
    <engine name="android" spec="^7.0.0" />
</widget>
